@model Object[]
@{
    ViewBag.Title = "MyJobs";
    Layout = "~/Views/Shared/_CustomLayout.cshtml";

    var session = ((JobSeekerWeb.CustomUtils.SessionObject)Model[0]);
    var jobList = (List<JobSeekerWeb.Models.job>)Model[1];
}

<main>
    <div class="my-jobs-main">
        <h3 class="title">
            My Jobs
        </h3>
        <div class="grid">
             
            @{
                foreach (JobSeekerWeb.Models.job item in jobList)
                {
                    Html.RenderPartial("MyJobsCard", item);
                }
            }

        </div>
    </div>
</main>
